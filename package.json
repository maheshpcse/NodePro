{
  "name": "nodepro",
  "version": "1.0.0",
  "description": "A Simple node server with mysql database connection",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "knex migrate:latest --knexfile=./src/config/knex.js"
  },
  "keywords": [
    "Akrivia"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onCommand:npm-intellisense.import"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "npm-intellisense",
      "properties": {
        "npm-intellisense.scanDevDependencies": true,
        "npm-intellisense.recursivePackageJsonLookup": true,
        "npm-intellisense.packageSubfoldersIntellisense": true,
        "npm-intellisense.showBuildInLibs": true,
        "npm-intellisense.importES6": false,
        "npm-intellisense.importQuotes": "'",
        "npm-intellisense.importLinebreak": ";\r\n",
        "npm-intellisense.importDeclarationType": "const"
      }
    },
    "commands": [
      {
        "command": "npm-intellisense.import",
        "title": "NPM Intellisense: Import module"
      }
    ]
  },
  "author": "Mahesh P",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "events": "^3.0.0",
    "express": "^4.17.1",
    "flatted": "^2.0.1",
    "form-data": "^3.0.0",
    "fs": "0.0.1-security",
    "image-to-base64": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.4",
    "knex-db-manager": "^0.5.0",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-base64-image": "^1.0.6",
    "nodemon": "^2.0.2",
    "objection": "^1.6.11",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "sessionstorage": "^0.1.0",
    "underscore": "^1.9.1"
  }
}
